@model Model.Kunde

@{
    ViewBag.Title = "Slett Kunde";
}


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
</head>
<body>
    <br /><br />
    <h3>Er du sikker på at du vil slette denne?</h3>
    <div>
        <h4>Kunde</h4>>
        <hr />
        <dl class="dl-horizontal">

            <dt>@Html.DisplayNameFor(model => model.id)</dt>
            <dd>@Html.DisplayFor(model => model.id)</dd>

            <dt>@Html.DisplayNameFor(model => model.Personnummer)</dt>
            <dd>@Html.DisplayFor(model => model.Personnummer)</dd>

            <dt>@Html.DisplayNameFor(model => model.Fornavn)</dt>
            <dd>@Html.DisplayFor(model => model.Fornavn)</dd>

            <dt>@Html.DisplayNameFor(model => model.Etternavn)</dt>
            <dd>@Html.DisplayFor(model => model.Etternavn)</dd>

            <dt>@Html.DisplayNameFor(model => model.Adresse)</dt>
            <dd>@Html.DisplayFor(model => model.Adresse)</dd>

            <dt>@Html.DisplayNameFor(model => model.Postnr)</dt>
            <dd>@Html.DisplayFor(model => model.Postnr)</dd>

            <dt>@Html.DisplayNameFor(model => model.Poststed)</dt>
            <dd>@Html.DisplayFor(model => model.Poststed)</dd>

        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()

            <div class="form-actions no-color">
                <input type="submit" value="Slett" class="btn btn-danger" />
                @Html.ActionLink("Kundeoversikt", "ListKunder","Kunde", new { @class = "btn btn-success" })
            </div>
        }
    </div>
</body>
</html>


